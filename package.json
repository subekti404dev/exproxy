{
  "name": "exproxy",
  "scripts": {
    "start": "bun server/index.js",
    "build": "ncc build server/index.js -o dist && bun run build:bin",
    "build:bin": "bun build ./dist/index.js --compile --outfile exproxy",
    "build:dashboard": "cd dashboard && bun run build && cd .. && mv dashboard/build server/_dashboard"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "aes256cbc-enc": "1.0.8",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "js-sha256": "^0.10.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.0"
  }
}